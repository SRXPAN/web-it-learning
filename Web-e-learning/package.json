{
  "name": "elearn-mvp-frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b --excludeFiles '**/__tests__/**' '**/*.test.*' && vite build",
    "build:production": "vite build",
    "build:cloudflare": "npm run build && cp _redirects dist/ && cp public/_headers dist/",
    "preview": "vite preview",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "lint": "tsc -b",
    "ci": "npm run lint && npm run test && npm run build"
  },
  "dependencies": {
    "@elearn/shared": "*",
    "clsx": "^2.1.1",
    "lucide-react": "^0.468.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.26.0",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@types/node": "^24.7.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.45",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.6.2",
    "vite": "^7.1.9",
    "vitest": "^1.6.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "jsdom": "^24.1.1"
  }
}
